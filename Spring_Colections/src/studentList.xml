<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="students" class="list.StudentList">
		<property name="students">
			<list>
				<ref bean="stu1" />
				<ref bean="stu2" />
				<ref bean="stu3" />
			</list>
		</property>
	</bean>
	<bean id="stu1" class="list.Student">
		<property name="name" value="raghu" />
		<property name="no" value="2" />
		<property name="marks" value="75" />
	</bean>

	<bean id="stu2" class="list.Student">
		<property name="name" value="varma" />
		<property name="no" value="22" />
		<property name="marks" value="12" />
	</bean>

	<bean id="stu3" class="list.Student">
		<property name="name" value="dantuluri" />
		<property name="no" value="222" />
		<property name="marks" value="63362" />
	</bean>
</beans>